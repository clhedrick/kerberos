[options]
    auth = force_first_pass no_ccache user_realm=%0

[run]
    authenticate = PAM_AUTHINFO_UNAVAIL

[output]
    NOTICE authentication failure; logname=%u uid=%i euid=%i tty= ruser= rhost=
